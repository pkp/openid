msgstr "Compléter l'inscription"

msgid "plugins.generic.openid.step2.complete"
msgstr ""
"S'il vous plaît, vérifiez les détails et ajouter toute information manquante"

msgid "plugins.generic.openid.step2.connect.btn"
msgstr "Se connecter et connecter les comptes"

msgid "plugins.generic.openid.step2.headline"
msgstr "Avez-vous déjà un compte utilisateur dans {$journalName} ?"

msgid "plugins.generic.openid.step2.choice.no"
msgstr "Non, je suis un-e nouveau-elle utilisateur-trice de {$journalName}."

msgid "plugins.generic.openid.step2.choice.yes"
msgstr "Oui, j'ai déjà un compte utilisateur."

msgid "plugins.generic.openid.step2.complete.btn"
msgstr "Inscription terminée"

msgid "plugins.generic.openid.step2.connect"
msgstr ""
"S'il vous plaît, connectez vous à votre compte sur {$journalName}.<br /"
">Votre information sera mise à jour automatiquement."

msgid "plugins.generic.openid.step2.connect.username"
msgstr "Nom d'utilisateur-trice ou courriel"

